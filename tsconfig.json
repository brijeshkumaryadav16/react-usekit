{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@react-usekit/templates": ["packages/templates/src"],
      "@react-usekit/config": ["packages/config"]
    }
  },
  "exclude": ["dist", "node_modules"]
}
