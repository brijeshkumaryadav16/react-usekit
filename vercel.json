{
  "framework": "nextjs",
  "buildCommand": "pnpm build:docs",
  "devCommand": "pnpm dev:docs",
  "installCommand": "pnpm install",
  "outputDirectory": "apps/docs/.next",
  "rootDirectory": "./",
  "redirects": [
    {
      "source": "/",
      "destination": "/docs",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
